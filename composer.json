{
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "description": "the \"Symfony Standard Edition\" distribution with the networking init CMS bundle configured",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "name": "networking/init-cms-sandbox",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "networking/elasticsearch-bundle": "4.x-dev",
    "networking/form-generator-bundle": "4.x-dev",
    "networking/init-cms-bundle": "4.x-dev",
    "symfony/flex": "^1.4",
    "symfony/proxy-manager-bridge": "^4.3"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^2.4",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "^4.0",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "^4.0",
    "behat/behat": "^3.0",
    "behat/symfony2-extension": "^2.1",
    "behat/mink-extension": "^2.1",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3"
  },
  "suggest": {
    "networking/elasticsearch-bundle": "Integrates elasticsearch into your CMS project"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/networking/elasticsearch-bundle.git",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/networking/form-generator-bundle.git",
      "no-api": true
    }
  ],
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "ckeditor:install --clear=drop": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "branch-alias": null,
    "symfony": {
      "allow-contrib": true
    }
  }
}
